{
    "name": "bloom-bolt-prisma",
    "version": "1.0.6",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "bolt": "npm run prepare && npx ts-node src/tests/bolt-example.ts",
        "prepare": "npm run build",
        "build": "tsc",
        "build:clean": "shx rm -rf ./dist ./coverage ./.nyc_output",
        "lint": "eslint --fix --ext .ts src",
        "mocha": "TS_NODE_PROJECT=tsconfig.json nyc mocha --config .mocharc.json \"src/**/*.spec.ts\"",
        "prisma": "npx prisma migrate dev --name init && npx prisma generate",
        "test": "npm run lint && npm run prisma && npm run mocha",
        "coverage": "codecov",
        "watch": "npx nodemon --watch 'src' --ext 'ts' --exec npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hackclub/professor-bloom.git",
        "directory": "bloom-bolt-prisma"
    },
    "keywords": [
        "Prisma",
        "OAuth",
        "Bolt",
        "Slack",
        "Slack Platform",
        "Slack Bot",
        "Slack API"
    ],
    "contributors": [
        {
            "name": "Kazuhiro Sera"
        },
        {
            "name": "V1ctorio"
        }
    ],
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^3.15.2",
        "@slack/logger": "^3.0.0",
        "@slack/oauth": "^2.6.3",
        "@types/node": "^17.0.45"
    },
    "devDependencies": {
        "@seratch_/bolt-extension-test-kit": "^1.0.5",
        "prisma": "^3.15.2",
        "typescript": "4.7.2",
            "@slack/bolt": "^3.12.2",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.42.1",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.4",
